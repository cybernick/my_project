<%= simple_form_for @article do |f| %>
  <%= f.input :title,:autofocus => true %>
  <%= f.input :article_description ,:autofocus => true %>
  <%= f.file_field :name_of_photo %>
  <div class ="text-center" %>
    <br><%= f.button :submit, 'Create article', class: "btn btn-large btn-primary" %></br>
  </div>
<% end %>
